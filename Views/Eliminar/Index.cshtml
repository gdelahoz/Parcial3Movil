
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Usuario.cshtml";
}

<h2 class="my-3">Eliminar telefono</h2>

<div class="row">
    <div class="text-danger">
        @ViewBag.Error
    </div>
    <form action="@Url.Content("~/Eliminar/Eliminar")" method="post">

        <div class="form-group col">
            <label>Numero a dar de baja</label><br />
            @Html.DropDownList("Origen", null, htmlAttributes: new { @class = "form-control", @Name = "NroTelefono" })
        </div>

        <div class="form-group mt-3">
            <input type="submit" value="Confirmar" class="btn btn-success" />
            <a href="~/Ingreso/Index" class="btn btn-danger">Cancelar</a>
        </div>
    </form>
</div>
